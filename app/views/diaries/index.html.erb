<%= render partial: "header" %>

<div class="index"></div>
<div class="diary_index">
  <div class="diary_main">

    <% @diaries.each do |diary| %>
    <div class="diary_list">

      <div class="date_nickname">
        <div class="date">
          <%= diary.date %>
        </div>
        <div class="nickname">
          <%= diary.user.nickname %>
        </div>
      </div>

      <div class="title">
        <%= diary.title %>
      </div>

      <div class="content">
        <%= diary.content %>
      </div>

    </div>
    <% end %>
  </div>
</div>